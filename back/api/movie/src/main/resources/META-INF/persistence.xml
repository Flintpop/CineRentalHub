      <class>mariadbPojo.Purchases</class>
      <class>mariadbPojo.Rentals</class>
      <class>mariadbPojo.ShoppingCart</class>
      <class>mariadbPojo.Users</class>
      <class>mariadbPojo.Views</class>
      <class>mariadbPojo.CommentsPojo</class>
      <class>mariadbPojo.ImagesPojo</class>
      <class>mariadbPojo.MoviesPojo</class>
      <class>mariadbPojo.PurchasesPojo</class>
